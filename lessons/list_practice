"""Practice with writing different types of lists."""

# Lines 4-5 add item to a list.
my_numbers: list[float] = []  # literal
my_numbers: list[float] = list()  # constructor

# Line add an item to a list.
my_numbers.append(1.5)

# Line 11 create an already populated list.
game_points: list[int] = [102, 86, 94]

# Line 14 using subscription notation.
game_points[2]

# Line 17 modifying by index.
game_points[1] = 72

# Len of lists.
print(len(game_points))

# Remove item from lists.
game_points.pop(1)


# Writing function of display.
def display(Input: list[int]) -> None:
    idx: int = 0
    while idx < len(Input):
        print(Input[idx])
        idx += 1


display(Input=game_points)
